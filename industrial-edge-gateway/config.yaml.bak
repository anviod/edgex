                strategy: periodic
                interval: 0s
            s7-water-meter:
                enable: false
                strategy: periodic
                interval: 0s
            slave-1:
                enable: true
                strategy: periodic
                interval: 10s
            slave-2:
                enable: true
                strategy: periodic
                interval: 30s
    opcua:
        - id: opcua-1
          name: Default OPC UA
          enable: true
          port: 5000
          endpoint: /test
          devices:
            opcua-device-1: false
            remote-slave-1: false
            rtu-device-1: false
            s7-pressure-sensor: true
            s7-pump-1: true
            s7-water-meter: true
            slave-1: true
            slave-2: false
    sparkplug_b:
        - id: sparkplug-1
          name: Default Sparkplug B
          enable: false
          client_id: ""
          group_id: ""
          node_id: ""
          enable_alias: false
          group_path: false
          offline_cache: false
          cache_mem_size: 0
          cache_disk_size: 0
          cache_resend_int: 0
          broker: ""
          port: 0
          username: ""
          password: ""
          ssl: false
          ca_cert: ""
          client_cert: ""
          client_key: ""
          key_password: ""
          devices: {}
channels:
    - id: s7-test-1
      name: S7 Test Channel
      protocol: s7
      enable: true
      config:
        ip: 192.168.1.100
        plcType: S7-1200
        port: 102
        rack: 0
        slot: 2
        startup: warm
      devices: []
    - id: modbus-tcp-
      name: OPC UA Simulator Channel
      protocol: opc-ua
      enable: false
      config:
        url: opc.tcp://127.0.0.1:4840
      devices:
        - id: opcua-device-1
          name: Simulated Machine A
          enable: true
          interval: 2s
          config: {}
          points:
            - id: machine_speed
              name: Machine Speed
              address: ns=2;s=Machine.Speed
              datatype: float32
              scale: 0
              offset: 0
              unit: rpm
              readwrite: RW
              group: ""
              report_mode: ""
              threshold: null
            - id: machine_status
              name: Machine Status
              address: ns=2;s=Machine.Status
              datatype: string
              scale: 0
              offset: 0
              unit: ""
              readwrite: R
              group: ""
              report_mode: ""
              threshold: null
            - id: machine_temp
              name: Core Temperature
              address: ns=2;s=Machine.Temp
              datatype: float32
              scale: 0
              offset: 0
              unit: °C
              readwrite: R
              group: ""
              report_mode: ""
