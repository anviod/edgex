          client_id: industrial_gateway_001
          topic: gateway/uplink
          username: ""
          password: ""
          devices:
            bacnet-2228316:
                enable: false
                strategy: periodic
                interval: 10s
            bacnet-2228317:
                enable: false
                strategy: periodic
                interval: 10s
            opcua-device-1:
