edge_rules:
    - id: 5700b238-4cf3-4f3b-b175-b29c585e860f
      name: TMPW
      type: state
      enable: true
      priority: 0
      trigger_mode: on_change
      source:
        alias: ""
        channel_id: ""
        device_id: ""
        point_id: ""
      sources:
        - alias: t1
          channel_id: modbus-tcp-1
          device_id: slave-1
          point_id: dev1_temp
        - alias: t2
          channel_id: modbus-tcp-1
          device_id: slave-2
          point_id: dev2_temp
      trigger_logic: AND
      condition: t1 > 1 && t2>3
      expression: t1 > 1 && t2>3
      actions:
        - type: log
          config: {}
        - type: mqtt
          config:
            message: TMPW
            send_strategy: single
            topic: gateway/warning
        - type: device_control
          config:
            _deviceList:
                - config:
                    slave_id: 1
                  enable: true
                  id: slave-1
                  interval: 5s
                  name: Slave Device 1
                  points:
                    - address: "40001"
                      datatype: int16
                      group: ""
                      id: dev1_temp
                      name: Temperature
                      offset: 0
                      readwrite: RW
                      report_mode: ""
                      scale: 0.1
                      threshold: null
                      unit: °C
                    - address: "40002"
                      datatype: int16
                      group: ""
                      id: dev1_humidity
                      name: Humidity
                      offset: 0
                      readwrite: RW
                      report_mode: ""
                      scale: 0.1
                      threshold: null
                      unit: '%'
                    - address: "40018"
